{
  "name": "modern-flexible",
  "version": "0.0.3",
  "packageManager": "pnpm@8.7.6",
  "description": "现代多设备伸缩布局方案",
  "type": "module",
  "keywords": [
    "flexiblejs",
    "responsive",
    "flexible"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/modern-flexible.git"
  },
  "author": "hemengke <https://github.com/hemengke1997>",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build:pages": "pnpm run build && cd playground/spa && pnpm run build",
    "test": "vitest",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint .",
    "up": "taze -I -w -r",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "bump": "bumpp package.json -c -p -t --all -x \"pnpm run changelog\""
  },
  "dependencies": {
    "deepmerge": "^4.3.1",
    "throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@minko-fe/eslint-config": "^1.3.6",
    "@minko-fe/tsconfig": "^1.3.6",
    "@types/throttle-debounce": "^5.0.0",
    "bumpp": "^9.2.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "8.50.0",
    "pnpm": "^8.7.6",
    "taze": "^0.11.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  }
}
